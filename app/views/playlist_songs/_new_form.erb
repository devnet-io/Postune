<%= form_for(@new_object, :url => user_playlist_playlist_songs_path) do |f| %>
	<%= render 'shared/form_error_messages' %>
	<div class="field">
		<%= f.label :title %>
		<%= f.text_field :title %>
	</div>
	<div class="field">
		<%= f.label :artist %>
		<%= f.text_field :artist %>
	</div>
	<div class="field">
		<%= f.label :album %>
		<%= f.text_field :album %>
	</div>
	<div class="field">
		<%= f.label :url %>
		<%= f.text_field :url %>
	</div>
	<div class="actions">
		<%= f.submit "Add" %>
	</div>
<% end %>
