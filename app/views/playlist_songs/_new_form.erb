<%= form_for @new_song, :url => url, :remote => remote do |f| %>
	<%= render 'shared/form_error_messages', :object => f.object %>
	<div class="field">
		<%= f.label :title %>
		<%= f.text_field :title %>
	</div>
	<div class="field">
		<%= f.label :artist %>
		<%= f.text_field :artist %>
	</div>
	<div class="field">
		<%= f.label :album %>
		<%= f.text_field :album %>
	</div>
	<div class="field">
		<%= f.label :url %>
		<%= f.text_field :url %>
	</div>
	<div class="actions">
		<%= f.submit "Add" %>
	</div>
<% end %>
